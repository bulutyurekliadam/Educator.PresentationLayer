@model List<Educator.EntityLayer.Concrete.Category>
@{
    Layout = null;
}

<div class="container">
    <br />
    <br />
    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Kategori Adı</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CategoryID</td>
                <td>@item.CategoryName</td>
            </tr>
        }
    </table>
    <a href="#" class="btn btn-outline-primary">Yeni Kategori Girişi  </a>
</div>